{
    "name": "laravel-liberu/measurement-units",
    "type": "library",
    "description": "Measurement units dependency for Laravel Liberu",
    "keywords": [
        "laravel-liberu",
        "measurement-units",
        "units"
    ],
    "homepage": "https://github.com/laravel-liberu/measurement-units",
    "license": "MIT",
    "authors": [
        {
            "name": "Mihai Ocneanu",
            "email": "mihai.ocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        },
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/forms": "^4.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/migrator": "^2.1",
        "laravel-liberu/select": "^4.0",
        "laravel-liberu/tables": "^4.0"
    },
    "autoload": {
